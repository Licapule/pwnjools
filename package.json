{
  "name": "pwnjools",
  "version": "0.1.0",
  "description": "Pwntools for Node.js",
  "main": "lib/",
  "scripts": {
    "build": "rimraf -rf dist && tsc -p tsconfig.ts",
    "format": "prettier --write \"{lib,test}/**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "prepublish": "npm run build",
    "publish": "npm publish",
    "test": "jest",
    "prerelease": "npm run build",
    "release": "release-it",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Licapule/pwnjools"
  },
  "keywords": [
    "pwn",
    "pwnjools",
    "pwntools",
    "js"
  ],
  "author": "Licapule",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Licapule/pwnjools/issues"
  },
  "homepage": "https://github.com/Licapule/pwnjools#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "prettier": "^2.7.1",
    "release-it": "^15.5.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4"
  }
}
